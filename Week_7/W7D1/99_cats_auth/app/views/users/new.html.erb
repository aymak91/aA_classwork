<h1>Sign Up</h1>

<form action="<%= users_url %>" method="post">
  <label for="user_name">Username</label>
  <input
    type="text"
    name="user[user_name]"
    id="user_name"
    value="Enter Username">

  <label for="password">Password</label>
  <input
    type="password"
    name="user[password]"
    id="password"
    value="Enter Password">

  <input type="submit" value="login">
</form>